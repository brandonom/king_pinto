{
  "name": "king-pinto",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "cd ./client && npm run dev",
    "seed:dev": "cd ./server && npm run seed:dev",
    "seed:prod": "cd ./server && npm run seed:prod",
    "dev:server": "cd ./server && npm run start:dev",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build": "cd ./client && NODE_ENV=development npm install --only=dev && npm run build",
    "install:client": "cd ./client && npm i",
    "install:server": "cd ./server && npm i",
    "install": "npm run install:client && npm run install:server",
    "start": "npm run dev"
  },
  "dependencies": {
    "@apollo/client": "^3.9.4",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "bootstrap": "^5.3.2",
    "bulma": "^0.9.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "bulma-carousel": "^4.0.24",
    "graphql": "^16.8.1",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "sass": "^1.70.0",
    "sort-by": "^0.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^5.0.8"
  }
}
